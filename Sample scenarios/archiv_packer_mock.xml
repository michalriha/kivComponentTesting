<?xml version="1.0" encoding="UTF-8" ?>
<project xmlns="http://www.kiv.zcu.cz/component-testing/mocker"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.kiv.zcu.cz/component-testing/mocker https://raw.github.com/michalriha/kivComponentTesting/master/UniMockerBindings/schema/unimocker.xsd">
  <simulated-components>
    <bundle symbolic-name="cz.zcu.kiv.kc.archiver" version="1.0.0">
      <service interface="cz.zcu.kiv.kc.archiver.IArchivePacker">
        <method name="getExtension">
          <invocation>
            <arguments />
            <return><String>7z</String></return>
          </invocation>
        </method>
        <method name="getFormatName">
          <invocation>
           <arguments />
           <return><String>7-Zip</String></return>
          </invocation>
        </method>
        <method name="packFilesToArchive">
          <invocation>
            <arguments>
              <argument ord-num="0">
                <AnyValue base-type="FileArrayList" />
              </argument>
              <argument ord-num="1">
                <AnyValue base-type="File" />
              </argument>
              <argument ord-num="2">
                <AnyValue base-type="File" />
              </argument>
            </arguments>
            <return><Boolean>false</Boolean></return>
          </invocation>
        </method>
      </service>
    </bundle>
  </simulated-components>
</project>