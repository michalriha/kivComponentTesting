<?xml version="1.0" encoding="UTF-8" ?>
<project xmlns="http://www.kiv.zcu.cz/component-testing/player"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.kiv.zcu.cz/component-testing/player https://raw.github.com/michalriha/kivComponentTesting/master/UniPlayerBindings/schema/uniplayer.xsd">
	<settings>
		<time-limit>75600</time-limit>
		<simul-step-delay>500</simul-step-delay>
	</settings>
	<actions xmlns:types="http://www.kiv.zcu.cz/component-testing/types"
		xmlns:xsit="http://www.w3.org/2001/XMLSchema-instance"
		xsit:schemaLocation="http://www.kiv.zcu.cz/component-testing/types https://raw.github.com/michalriha/kivComponentTesting/master/CommonTypes/schema/types.xsd">
		<action time="100">
			<recurrence count="3">
				<equidistant step="100" />
			</recurrence>
			<command>
				<event topic="changeSourceDir" key="dir">
					<argument>
						<types:String>d:/Obrázky/</types:String>
					</argument>
				</event>
			</command>
		</action>
		<action time="150">
			<recurrence count="4">
				<equidistant step="100" />
			</recurrence>
			<command>
				<event topic="changeSourceDir" key="dir">
					<argument>
						<types:String>d:/</types:String>
					</argument>
				</event>
			</command>
		</action>
		<action time="150">
			<recurrence count="5">
				<exponential rate="5" time-span="900" />
			</recurrence>
			<command>
				<event topic="changeDestinationDir" key="dir">
					<argument>
						<types:String>e:/</types:String>
					</argument>
				</event>
			</command>
		</action>
		<action time="200">
			<recurrence count="6">
				<exponential rate="10" time-span="900" />
			</recurrence>
			<command>
				<event topic="changeDestinationDir" key="dir">
					<argument>
						<types:String>c:/</types:String>
					</argument>
				</event>
			</command>
		</action>
		<action time="154">
			<recurrence count="2">
				<equidistant step="15" />
			</recurrence>
			<command>
				<call service="cz.zcu.kiv.kc.interfaces.ICopyPlugin" method="executeAction">
					<arguments>
						<argument ord-num="0">
							<types:Files>
								<types:item ord-num="0">
									<types:File>d:/676956.iso</types:File>
								</types:item>
							</types:Files>
						</argument>
						<argument ord-num="1">
							<types:String>e:/tmp/nazdar</types:String>
						</argument>
						<argument ord-num="2">
							<types:String>d:/</types:String>
						</argument>
					</arguments>
				</call>
			</command>
		</action>
		<action time="150">
			<recurrence count="1">
				<equidistant step="15" />
			</recurrence>
			<command>
				<call service="cz.zcu.kiv.kc.interfaces.IViewPlugin" method="executeAction">
					<arguments>
						<argument ord-num="0">
							<types:Files>
								<types:item ord-num="0">
									<types:File>d:/Stažené/mips2.asm</types:File>
								</types:item>
							</types:Files>
						</argument>
						<argument ord-num="1">
							<types:String>d:/Stažené/</types:String>
						</argument>
						<argument ord-num="2">
							<types:String>d:/Stažené/</types:String>
						</argument>
					</arguments>
				</call>
			</command>
		</action>
		<action time="700">
			<recurrence count="1">
				<equidistant step="15" />
			</recurrence>
			<command>
				<call service="cz.zcu.kiv.kc.interfaces.IDeletePlugin" method="executeAction">
					<arguments>
						<argument ord-num="0">
							<types:Files>
								<types:item ord-num="0">
									<types:File>e:/tmp/nazdar/</types:File>
								</types:item>
							</types:Files>
						</argument>
						<argument ord-num="1">
							<types:String>d:/</types:String>
						</argument>
						<argument ord-num="2">
							<types:String>e:/tmp/</types:String>
						</argument>
					</arguments>
				</call>
			</command>
		</action>
	</actions>
</project>
