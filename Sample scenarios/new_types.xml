<?xml version="1.0" encoding="UTF-8" ?>
<project xmlns="http://www.kiv.zcu.cz/component-testing/mocker"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.kiv.zcu.cz/component-testing/mocker https://raw.github.com/michalriha/kivComponentTesting/extract_common_datatypes/UniMockerBindings/schema/unimocker.xsd">
    <simulated-components
       xmlns:types="http://www.kiv.zcu.cz/component-testing/types"
       xmlns:xsit="http://www.w3.org/2001/XMLSchema-instance"
       xsit:schemaLocation="http://www.kiv.zcu.cz/component-testing/types https://raw.github.com/michalriha/kivComponentTesting/extract_common_datatypes/CommonTypes/schema/types.xsd"
    >
        <bundle symbolic-name="cz.zcu.kivcmd.common" version="1.0.0">
            <service interface="cz.zcu.kiv.kc.plugin.Plugin" ignore-undefined-methods="true" ignore-undefined-possibilities="true">
                <method name="executeAction">
                    <invocation>
                        <arguments>
                            <argument ord-num="0">
                                <types:Files type="array">
								    <types:item ord-num="0"><types:File>d:\obrazky.zip2</types:File></types:item>
								    <types:item ord-num="1"><types:File>d:\obrazky.zip1</types:File></types:item>
								</types:Files>
                            </argument>
                            <argument ord-num="1">
                                <AnyValue base-type="String" />
                            </argument>
                            <argument ord-num="2">
                                <AnyValue base-type="String" />
                            </argument>
                            <argument ord-num="3">
                                <types:File>d:\obrazky.zip2</types:File>
                            </argument>
                            <argument ord-num="4">
                                <types:Ints>
                                    <types:item ord-num="0"><types:Integer>123456</types:Integer></types:item>
                                    <types:item ord-num="1"><types:Integer>654321</types:Integer></types:item>
                                    <types:item ord-num="2"><types:Integer>987654</types:Integer></types:item>
                                    <types:item ord-num="3"><types:Integer>456789</types:Integer></types:item>
                                </types:Ints>
                            </argument>
                        </arguments>
                    </invocation>
                </method>
            </service>
        </bundle>
    </simulated-components>
</project>